package com.epro.comp.im.mvp.model.bean;


public class ChatMessage<T extends MsgBody> {
    private String uuid;
    private String msgId;
    private MsgType msgType;
    private T body;
    private MsgSendStatus sentStatus;
    private String csId; //客服ID
    private String csName;//客服名称
    private String csAvatar;//客服头像
    private String customerId;//客户ID
    private String customerAvatar;//客户头像
    private String customerName;//客户昵称
    private boolean isSend=true; // true 是发送消息  false 是接收消息
    private long sentTime;//发送接收时间
    private boolean isRead=false;// true 已读 false 未读
    private boolean isOffline=false; // true 离线消息 false 在线消息
    private String shopId; //店铺ID
    private String shopLogo;//店铺logo
    private String shopName;//店铺名称


    public String getUuid() {
        return uuid;
    }

    public void setUuid(String uuid) {
        this.uuid = uuid;
    }

    public String getMsgId() {
        return msgId;
    }

    public void setMsgId(String msgId) {
        this.msgId = msgId;
    }

    public MsgType getMsgType() {
        return msgType;
    }

    public void setMsgType(MsgType msgType) {
        this.msgType = msgType;
    }

    public T getBody() {
        return body;
    }

    public void setBody(T body) {
        this.body = body;
    }

    public String getCsId() {
        return csId;
    }

    public void setCsId(String csId) {
        this.csId = csId;
    }

    public String getCustomerId() {
        return customerId;
    }

    public void setCustomerId(String customerId) {
        this.customerId = customerId;
    }

    public MsgSendStatus getSentStatus() {
        return sentStatus;
    }

    public void setSentStatus(MsgSendStatus sentStatus) {
        this.sentStatus = sentStatus;
    }


    public long getSentTime() {
        return sentTime;
    }

    public void setSentTime(long sentTime) {
        this.sentTime = sentTime;
    }

    public boolean isSend() {
        return isSend;
    }

    public void setSend(boolean send) {
        isSend = send;
    }

    public void setRead(boolean read) {
        isRead = read;
    }

    public boolean isRead() {
        return isRead;
    }

    public void setOffline(boolean offline) {
        isOffline = offline;
    }

    public boolean isOffline() {
        return isOffline;
    }

    public void setShopId(String shopId) {
        this.shopId = shopId;
    }

    public String getShopId() {
        return shopId;
    }
}
